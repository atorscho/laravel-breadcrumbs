{
    "name": "atorscho/crumbs",
    "description": "Breadcrumbs package for Laravel 5.",
    "keywords": [
        "breadcrumbs",
        "crumbs",
        "navigation",
        "laravel"
    ],
    "homepage": "https://alextorscho.com/docs/crumbs",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Torscho",
            "email": "contact@alextorscho.com",
            "homepage": "http://alextorscho.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "^6 || ^7 || ^8"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "Atorscho\\Crumbs\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Atorscho\\Crumbs\\CrumbsServiceProvider"
            ]
        }
    }
}
